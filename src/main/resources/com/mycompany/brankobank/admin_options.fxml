            <Label text="Branco Bank- Administration Section(Main Branch)">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <Label text="Please Choose From the options below" />
         </children>
      </VBox>
      <VBox alignment="CENTER" layoutX="250.0" layoutY="122.0" prefHeight="200.0" prefWidth="100.0" spacing="20.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Button mnemonicParsing="false" onAction="#ManageUserClick" text="Manage Users">
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </Button>
            <Button mnemonicParsing="false" onAction="#ManageEmployeeClick" text="Manage Employees">
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </Button>
         </children>
      </VBox>
   </children>
